{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://127.0.0.1:8100",
        "Protocols":"Http1AndHttp2AndHttp3"
      },
      "Https": {
        "Url": "https://127.0.0.1:8101",
        "Protocols":"Http1AndHttp2AndHttp3"
      }
    }
  },
  "WebComponentsServer": {
    "Root": "../..",
    "WebComponents": {
      "Vanilla": {
        "FileProvider": {
          "FilePath": "Components/Vanilla/Web-Components/dist",
          "BaseUrl": "vanilla"
        }
      },
      "React": {
        "FileProvider": {
          "FilePath": "Components/React/Web-Components/dist",
          "BaseUrl": "react"
        }
      },
      "Angular": {
        "FileProvider": {
          "FilePath": "Components/Angular/Web-Components/dist/web-component",
          "BaseUrl": "angular"
        }
      },
      "AppDemo1": {
        "FileProvider": {
          "FilePath": "Apps/appdemo1/dist",
          "BaseUrl": "appdemo1"
        }
      }
    }
  }
}
